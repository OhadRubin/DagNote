{
    "name": "dag-note-editor",
    "displayName": "DAG Note Editor",
    "description": "A DAG Note Editor for VS Code",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:dagNoteEditor.open"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [{
            "command": "dagNoteEditor.open",
            "title": "Open DAG Note Editor"
        }]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./ && webpack --config webpack.config.js",
        "watch": "tsc -watch -p ./ &&  webpack --mode development --watch",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js",
        "start": "react-scripts start",
        "build": "react-scripts build"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.2.2",
        "@types/node": "14.x",
        "@types/react": "^18.3.11",
        "@types/react-dom": "^18.3.1",
        "@types/vscode": "^1.60.0",
        "@typescript-eslint/eslint-plugin": "^4.26.0",
        "@typescript-eslint/parser": "^4.26.0",
        "css-loader": "^5.2.6",
        "eslint": "^7.27.0",
        "glob": "^7.1.7",
        "mocha": "^8.4.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-scripts": "5.0.1",
        "style-loader": "^2.0.0",
        "ts-loader": "^9.2.2",
        "typescript": "^4.3.2",
        "vscode-test": "^1.5.2",
        "webpack": "^5.0.2",
        "webpack-cli": "^4.7.0"
    },
    "dependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}